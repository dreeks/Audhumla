<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
      <children>
         <Button fx:id="previousButton" mnemonicParsing="false" styleClass="button-profile-selector" text="&lt;">
            <font>
               <Font size="20.0" />
            </font>
            <HBox.margin>
               <Insets left="20.0" right="20.0" />
            </HBox.margin>
         </Button>
         <Label fx:id="selectedProfileLabel" alignment="CENTER" contentDisplay="CENTER" maxWidth="350.0" minWidth="350.0" prefWidth="350.0" text="1.16.3" textFill="WHITE" HBox.hgrow="ALWAYS">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
         <Button fx:id="nextButton" mnemonicParsing="false" styleClass="button-profile-selector" text="&gt;">
            <font>
               <Font size="20.0" />
            </font>
            <HBox.margin>
               <Insets left="20.0" right="20.0" />
            </HBox.margin>
         </Button>
      </children>
      <padding>
         <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
      </padding>
</fx:root>
